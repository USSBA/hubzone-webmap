<%
  unless qualification["expires"].nil?
    expires_text = qualification["expires"].to_date < Date.today ? "expired" : "expires"
  end
%>
<tr>
  <th id="<%= qualification['hz_type'] %>">
    <div class="qualification-label"><%= t(('hubzone_assertions.' + qualification["hz_type"]).to_s)  %></div>
    <% unless qualification["expires"].nil? %>
    <span class="expires-label"><%= t('hubzone_assertions.' + expires_text) %> <%= qualification["expires"].to_s %></span>
    <% end %>
  </th>
</tr>
