<div class="legend-header open">
  <h4><%= t('legend.title') %></h4>
  <i class="legend-button fa fa-chevron-down"></i>
</div>
<fieldset class="usa-fieldset-inputs">
  <legend class="usa-sr-only"><%= t('legend.title') %></legend>
  <ul class="legend-content">
    <div class="legend-list-title"><%= t('legend.qualified') %></div>
    <li class="legend-item">
      <input id="qct" type="checkbox" name="qct" value="qct" checked="">
        <label for="qct">
          <div class="legend-layer-symbols layer-qct"></div><%= t('legend.qct') %>
        </label>
      </input>
    </li>
    <li class="legend-item">
      <input id="qnmc" type="checkbox" name="qnmc" value="qnmc" checked="">
      <label for="qnmc">
        <div class="legend-layer-symbols layer-qnmc"></div><%= t('legend.qnmc') %>
      </label>
    </li>
    <li class="legend-item">
      <input id="indian_lands" type="checkbox" name="indian_lands" value="indian_lands" checked="">
      <label for="indian_lands">
        <div class="legend-layer-symbols layer-indian_lands"></div><%= t('legend.indian_lands') %>
      </label>
    </li>
    <div class="legend-list-title"><%= t('legend.expiring') %></div>
    <li class="legend-item">
      <input id="redesignated" type="checkbox" name="redesignated" value="redesignated" checked="">
      <label for="redesignated">
        <div class="legend-layer-symbols layer-qct_r"></div>
        <div class="legend-layer-symbols layer-qnmc_r"></div><%= t('legend.redesignated') %>
      </label>
    </li>
    <li class="legend-item">
      <input id="qda" type="checkbox" name="qda" value="qda" checked="">
      <label for="qda">
        <div class="legend-layer-symbols layer-qct_qda"></div>
        <div class="legend-layer-symbols layer-qnmc_qda"></div><%= t('legend.qda') %>
      </label>
    </li>
    <li class="legend-item">
      <input id="brac" type="checkbox" name="brac" value="brac" checked="">
      <label for="brac">
        <div class="legend-layer-symbols layer-qct_b"></div>
        <div class="legend-layer-symbols layer-qnmc_brac"></div>
        <div class="legend-layer-symbols layer-brac"></div><%= t('legend.brac') %>
      </label>
    </li>
  </ul>
</fieldset>
